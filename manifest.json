{
    "manifest_version": 3,
    "name": "Jinx",
    "description": "Changes site images to Jinx",
    "version": "1.1.4",
    "action": {
      "default_popup": "jinx.html",
      "default_icon": "jinx2.png"
    },
    "icons": {
    "16": "jinx2.png",
    "32": "jinx2.png",
    "48": "jinx2.png",
    "128": "jinx2.png"
    },
    "content_scripts": [{
        "js": ["main.js"],
        "matches": ["<all_urls>"]
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["activeTab"]
  }