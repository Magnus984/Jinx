{
    "manifest_version":2,
    "version":"1.0",
    "name":"Jinx",
    "description": "Changes the images of webpagesto Jinx",
    "content_scripts":[
     {
      "matches":["<all_urls>"],
      "js":["main.js"]
     }
    ],
    "browser_action": {
      "default_popup": "jinx.html",
      "default_icon": "jinx2.png"
    },
    "icons": {
    "16": "jinx2.png",
    "32": "jinx2.png",
    "48": "jinx2.png",
    "128": "jinx2.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["tabs"]
  }